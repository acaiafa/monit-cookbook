# <%= @config.instance_name %> check generated by Chef
check <%= @config.check %> <%= @config.instance_name %>
<% if @config.with %>
  with <%= @config.with %>
<% end %>
<% if @config.every %>
  every <%= @config.every %>
<% end %>
<% if @config.group %>
  group <%= @config.group %>
<% end %>
<% if @config.start_program %>
  start "<%= @config.start_program %>"
  <%- if @config.user -%>
    as uid <%= @config.user %>
  <%- end -%>
<% end %>
<% if @config.stop_program %>
  stop "<%= @config.stop_program %>"
  <%- if @config.user -%>
    as uid <%= @config.user %>
  <%- end -%>
<% end %>
<% if @config.mode %>
  mode <%= @config.mode %>
<% end %>
<% if @config.test_conditions %>
  <% @config.test_conditions.each do |test| %>
    <%= test %>
<% end %>
<% end %>
